{
  "name": "root",
  "private": true,
  "workspaces": [
    "apps/**/*",
    "recipes/**/*",
    "recipes-packages/**/*"
  ],
  "scripts": {
    "_:package:bootstrap": "npx lerna bootstrap",
    "_:package:clean": "npx lerna clean --y && npx rimraf ./node_modules/",
    "_:package:publish": "lerna publish from-package --y --no-verify-access",
    "_:package:reset": "npm run _:package:clean && npm run _:package:bootstrap",
    "_:package:update": "ncu -u --packageFile \"(**/*/package.json|package.json)",
    "_:sort:package": "sort-package-json \"(recipes|recipes-packages|apps|apps-packages)/**/package.json\" \"package.json\"",
    "build": "lerna run build",
    "lint:es": "eslint --fix ./**/*.{ts,tsx,js,jsx,json} --ignore-pattern **/*.d.ts",
    "lint:scss": "stylelint --fix ./**/*.scss",
    "test": "lerna run test"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.92.0",
    "@aws-cdk/aws-cloudfront": "1.92.0",
    "@aws-cdk/aws-route53": "1.92.0",
    "@aws-cdk/aws-s3": "1.92.0",
    "@aws-cdk/aws-s3-deployment": "1.92.0",
    "@aws-cdk/core": "1.92.0",
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.2.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.32",
    "autoprefixer": "^10.2.5",
    "aws-cdk": "1.92.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
